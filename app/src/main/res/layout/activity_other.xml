<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/label_bg">
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:background="@color/white">
        <RelativeLayout
            android:id="@+id/other_feedback_layout"
            android:paddingLeft="@dimen/dp_thirty"
            android:background="@color/white"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_fourty_eight"
            android:paddingRight="@dimen/dp_twenty">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/other_feedback"
                android:textColor="@color/main_text_color"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_forteen"/>
            <ImageView
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right_arrow"/>

        </RelativeLayout>
        <View
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_one"
            android:layout_marginLeft="@dimen/dp_thirty"
            android:background="@color/split_color"/>
        <RelativeLayout
            android:id="@+id/other_link_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_fourty_eight"
            android:paddingLeft="@dimen/dp_thirty"
            android:paddingRight="@dimen/dp_twenty">
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/other_link"
                android:textColor="@color/main_text_color"
                android:layout_centerVertical="true"
                android:textSize="@dimen/sp_forteen"/>
            <TextView
                android:id="@+id/other_tel_tv"
                android:layout_centerVertical="true"
                android:layout_toLeftOf="@+id/pc_age_right_img"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="@dimen/dp_ten"
                android:textColor="@color/main_text_color"
                android:textSize="@dimen/sp_forteen"/>
            <ImageView
                android:id="@+id/pc_age_right_img"
                android:layout_alignParentRight="true"
                android:layout_centerVertical="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/right_arrow"/>
        </RelativeLayout>


    </LinearLayout>

</LinearLayout>